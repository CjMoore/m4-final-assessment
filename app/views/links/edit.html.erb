<div class="row">
  <div class="col s3">
    <p></p>
  </div>
  <div class="col s6">
    <div class="card" id="link-form">
      <div class="card-content">
        <%= form_for @link do |f| %>
          <%= f.label :url %>
          <%= f.text_field :url %>
          <%= f.label :title %>
          <%= f.text_field :title %>
          <%= f.submit  class: 'btn'%>
        <% end %>
      </div>
    </div>
  </div>
</div>
